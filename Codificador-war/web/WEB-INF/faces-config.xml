<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>Login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/factura/iFactura.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>facturaView</from-outcome>
            <to-view-id>/factura/iFacturaView.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>pago</from-outcome>
            <to-view-id>/iPago.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>rpago</from-outcome>
            <to-view-id>/iReportePago.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
